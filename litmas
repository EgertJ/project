import random
print("!!!! AINULT INGLISEKEELSED SÕNAD!!!!")
käike=10
def genereeri_sõna(infile):
    random_sõna= random.choice(open(infile).read().split("\n"))
    return random_sõna
def peamine():
    infile="sõnad.txt"
    print(genereeri_sõna(infile))

def küsimus():
    täht=input("Mis tähte Sa pakud?:")
küsimus()
    


